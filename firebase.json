{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "webapp/dist",
    "redirects": [
      {
        "source": "/",
        "destination": "https://www.hubbub.live",
        "type": 302
      },
      {
        "source": "/privacy",
        "destination": "https://www.iubenda.com/privacy-policy/8115331",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "/oauth",
        "destination": "/assets/oauth.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
